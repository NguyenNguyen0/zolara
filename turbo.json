{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build:dev": {
      "cache": false,
      "inputs": ["$TURBO_DEFAULT$", ".env*", "app.config.js", "eas.json"]
    },
    "build:preview": {
      "cache": false,
      "inputs": ["$TURBO_DEFAULT$", ".env*", "app.config.js", "eas.json"]
    },
    "build:production": {
      "cache": false,
      "inputs": ["$TURBO_DEFAULT$", ".env*", "app.config.js", "eas.json"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
